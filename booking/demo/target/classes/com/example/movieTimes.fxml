<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.MovieTimesController">
   <children>
      <Text fx:id="movieText" strokeType="OUTSIDE" strokeWidth="0.0" text="Movie: " wrappingWidth="70.625" />
      <ComboBox fx:id="movieTimes" prefWidth="150.0" />
      <Button mnemonicParsing="false" text="Button" onAction="#switchToBooking" />
   </children>
</VBox>
